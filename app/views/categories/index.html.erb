<h1 class="page-header center-to-left">All Event Categories</h1>
<div class="center-to-left" ">
<%= will_paginate %>
<% @categories.each do |category| %>
    <ul class="category-listing">
        <div class="row">
            <div class="well col-md-4 col-md-offset-4">
                <li class="category-title"><%= link_to "#{category.name}", category_path(category) %></li>
            </div>
        </div>
    </ul>

<% end %>
<%= will_paginate %>
</div>

